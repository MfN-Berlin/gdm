<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="remove fields from networks" author="thomas.pfuhl">
        <preConditions onFail="CONTINUE">
                <columnExists columnName="headquarter" tableName="networks" schemaName="projektmetadaten"/>
        </preConditions>

        <comment>remove properties</comment>
        <dropColumn schemaName="projektmetadaten" tableName="networks">
            <column name="headquarter" type="varchar(100)"/>
        </dropColumn>
        
    </changeSet>
 
</databaseChangeLog>
